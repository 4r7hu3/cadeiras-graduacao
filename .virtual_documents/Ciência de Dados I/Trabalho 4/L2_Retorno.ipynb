


# Crie a função aqui.
def retorno(precos):
  retornos = []

  for i in range(1, len(precos)):
    retorno = (precos[i] / precos[i-1]) - 1
    retornos.append(retorno)

  return retornos


## Casos de teste. Não modificar.

print(retorno([37.8, 36.8, 38.0, 39.5]))
print(retorno([100.20, 98.80, 99.50, 102.80, 107.20]))


