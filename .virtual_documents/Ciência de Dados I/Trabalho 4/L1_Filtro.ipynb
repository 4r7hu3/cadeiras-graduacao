


# Crie a função aqui.
def filtro(l):
  l_copia = l.copy()

  for i in l:
    if i % 3 == 0:
      l_copia.remove(i)

  return l_copia


## Casos de teste. Não modificar.

print(filtro([2,3,6]))
print(filtro([0,0,1,0]))
print(filtro([2,4,2,6,2,0,2,2]))

# Saída esperada
# [2]
# [1]
# [2, 4, 2, 2, 2, 2]

