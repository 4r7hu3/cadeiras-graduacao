


# Crie a função aqui.
def acumulada(l):
  ac = l

  for i in range(1, len(l)):
    ac[i] = ac[i] + ac[i-1]

  return ac


## Casos de teste. Não modificar.

print(acumulada([1, 2, 3, 4, 5]))
print(acumulada([10, -20, -30, 40, 50]))
print(acumulada([-10, -20, -40, 60]))

# Saída esperada:
# [1, 3, 6, 10, 15]
# [10, -10, -40, 0, 50]
# [-10, -30, -70, -10]
