


# Crie a função aqui.
def media_acumulada(l):
  m_acum = [l[0]]
  cont_mes = 2

  for i in range(1, len(l)):
    l[i] += l[i-1]
    m_acum.append(l[i]/cont_mes)
    cont_mes += 1

  return m_acum


## Casos de teste. Não modificar.

print(media_acumulada([1, 2, 3, 4, 5]))
print(media_acumulada([10, -20, -30, 40, 50]))
print(media_acumulada([-10, -20, -40, 60]))

# Saída esperada:
# [1, 1.5, 2, 2.5, 3]
# [10, -5, -13.33, 0, 10]
# [-10, -15, -23.33, -2.5]
