


# Crie a função aqui.
def media_movel_exp(l, d):
  alpha = 2/(d+1)
  soma_s = 0
  media_e = []

  for i in l[:d]:
    soma_s += i

  mms = soma_s/d
  media_e.append(mms)

  for i in range(1, len(l) - d + 1):
    anterior = media_e[i-1] * (1-alpha)
    atual = l[i+d-1] * alpha
    media_e.append(anterior + atual)

  return media_e




